
library(shiny)
library(shinydashboard)
library(shinydashboardPlus)

shinyApp(
  ui = dashboardPage(
    dashboardHeader(),
    dashboardSidebar(),
    dashboardBody(
      box(
        title = "Star example",
        fluidRow(
          "python                   ",
          starBlock(4)
        ),
        starBlock(5),
        starBlock(5, color = "olive"),
        starBlock(1, color = "maroon"),
        starBlock(3, color = "teal")
      )
    ),
    title = "starBlock"
  ),
  server = function(input, output) { }
)